@model List<WebShop12rus.ViewModels.EmployeeView>

    <h2>Список сотрудников</h2>
    <div>
        <table>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.ActionLink(@item.Id.ToString(),"Details", new { id = item.Id })
                    </td>
                    <td>@item.LastName</td>
                    <td>@item.FirstName</td>
                    <td>@item.Patronymic</td>
                </tr>
            }
        </table>
    </div>
